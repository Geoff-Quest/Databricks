);

ALTER TABLE MOVIE_HUB
	ADD CONSTRAINT PK_MOVIE_HUB PRIMARY KEY (MOVIE_HSH, MOVIE_HKEY);

CREATE TABLE MOVIE_SAT
(
	MOVIE_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	MOVIE_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	MOVIE_NUMBER DEC,
	MOVIE_TITLE VARCHAR(20),
	MOVIE_DIRECTOR VARCHAR(20),
	DESCRIPTION VARCHAR(20),
	STAR_1_NAME VARCHAR(20),
	RATING VARCHAR(50),
	STAR_2_NAME VARCHAR(20),
	GENRE VARCHAR(8000),
	RENTAL_RATE DEC,
	MOVIE_URL VARCHAR(50),
	MOVIE_CLIP VARCHAR(8000),
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	MOVIE_SAT_RSRC CHAR(256),
	MOVIE_HKEY CHAR(32) NOT NULL
);

ALTER TABLE MOVIE_SAT
	ADD CONSTRAINT PK_MOVIE_SAT PRIMARY KEY (LOAD_DTS, MOVIE_HSH, LDTS, RSRC, MOVIE_HKEY);

