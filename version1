	ADD CONSTRAINT FK_PAYMENT_LNK_CUST_HUB FOREIGN KEY (CUST_HSH, CUST_HKEY) REFERENCES CUST_HUB (CUST_HSH, CUST_HKEY);

ALTER TABLE PAYMENT_LNK
