
CREATE TABLE CUST_CREDIT_HUB
(
	CUST_CREDIT_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	CUST_NUMBER DEC,
	CUST_CREDIT_HKEY CHAR(32) NOT NULL,
	CUST_CREDIT_LDTS TIMESTAMP,
	CUST_CREDIT_RSRC CHAR(256)
);

ALTER TABLE CUST_CREDIT_HUB
	ADD CONSTRAINT PK_CUST_CREDIT_HUB PRIMARY KEY (CUST_CREDIT_HSH, CUST_CREDIT_HKEY);

CREATE TABLE CUST_CREDIT_SAT
(
	CUST_CREDIT_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	CUST_CREDIT_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	CUST_NUMBER DEC,
	CREDIT_CARD DEC,
	CREDIT_CARD_EXP DEC,
	STATUS_CODE VARCHAR(20),
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	CUST_CREDIT_SAT_RSRC CHAR(256),
	CUST_CREDIT_HKEY CHAR(32) NOT NULL
);

ALTER TABLE CUST_CREDIT_SAT
	ADD CONSTRAINT PK_CUST_CREDIT_SAT PRIMARY KEY (CUST_CREDIT_HSH, LOAD_DTS, LDTS, RSRC, CUST_CREDIT_HKEY);

CREATE TABLE CUST_HUB
(
	CUST_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	CUST_NUMBER DEC,
	CUST_HKEY CHAR(32) NOT NULL,
	CUST_LDTS TIMESTAMP,
	CUST_RSRC CHAR(256)
);

ALTER TABLE CUST_HUB
	ADD CONSTRAINT PK_CUST_HUB PRIMARY KEY (CUST_HSH, CUST_HKEY);

CREATE TABLE CUST_SAT
(
	CUST_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	CUST_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	CUST_NUMBER DEC,
	CUST_ADDRESS VARCHAR(20),
	CUST_CITY VARCHAR(20),
	CUST_FIRST_NAME VARCHAR(20),
	CUST_LAST_NAME VARCHAR(20),
	CUST_STATE VARCHAR(20),
	CUST_ZIP_CODE DEC,
	EMAIL VARCHAR(50),
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	CUST_SAT_RSRC CHAR(256),
	CUST_HKEY CHAR(32) NOT NULL
);

ALTER TABLE CUST_SAT
	ADD CONSTRAINT PK_CUST_SAT PRIMARY KEY (CUST_HSH, LOAD_DTS, LDTS, RSRC, CUST_HKEY);

CREATE TABLE EMP_HUB
(
	EMP_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	EMP_NUMBER VARCHAR(20),
	EMP_PHONE DEC,
	EMP_HKEY CHAR(32),
	EMP_LDTS TIMESTAMP,
	EMP_RSRC CHAR(256)
);

ALTER TABLE EMP_HUB
	ADD CONSTRAINT PK_EMP_HUB PRIMARY KEY (EMP_HSH);

CREATE TABLE EMP_LNK
(
	LNK_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	EMP_HSH VARCHAR(8000),
	STORE_HSH VARCHAR(8000),
	STORE_HKEY CHAR(32),
	EMP_HKEY CHAR(32) NOT NULL,
	EMP_LDTS TIMESTAMP,
	EMP_RSRC CHAR(256)
);

ALTER TABLE EMP_LNK
	ADD CONSTRAINT PK_EMP_LNK PRIMARY KEY (LNK_HSH, EMP_HKEY);

CREATE TABLE EMP_SAT
(
	EMP_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	EMP_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	EMP_NUMBER VARCHAR(20),
	SUPERVISOR VARCHAR(20),
	EMP_FIRST_NAME VARCHAR(20),
	EMP_ADDRESS VARCHAR(20),
	EMP_PHONE DEC,
	EMP_ADDRESS_2 VARCHAR(20),
	SOC_SEC_NUMBER DEC,
	HIRE_DATE VARCHAR(8000),
	SALARY DEC,
	EMAIL VARCHAR(20),
	STORE_NUMBER DEC,
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	EMP_SAT_RSRC CHAR(256)
);

ALTER TABLE EMP_SAT
	ADD CONSTRAINT PK_EMP_SAT PRIMARY KEY (EMP_HSH, LOAD_DTS, LDTS, RSRC);

CREATE TABLE MO_RENT_REC_HUB
(
	MO_RENT_REC_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	MOVIE_NUMBER DEC,
	MO_RENT_REC_HKEY CHAR(32),
