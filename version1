	ADD CONSTRAINT PK_MOVIE_STORE_LNK PRIMARY KEY (LNK_HSH, MOVIE_STORE_HKEY);

CREATE TABLE PAYMENT_HUB
(
	PAYMENT_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	PAYMENT_TRANSACTION_NUMBER DEC,
	PAYMENT_HUB_HKEY CHAR(32) NOT NULL,
	PAYMENT_HUB_LDTS TIMESTAMP,
	PAYMENT_HUB_RSRC CHAR(256)
);

ALTER TABLE PAYMENT_HUB
	ADD CONSTRAINT PK_PAYMENT_HUB PRIMARY KEY (PAYMENT_HSH, PAYMENT_HUB_HKEY);

CREATE TABLE PAYMENT_LNK
(
	LNK_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	PAYMENT_HSH VARCHAR(8000),
	CUST_CREDIT_HSH VARCHAR(8000),
	CUST_HSH VARCHAR(8000),
	EMP_HSH VARCHAR(8000),
	PAYMENT_HUB_HKEY CHAR(32),
	PAYMENT_HKEY CHAR(32) NOT NULL,
	PAYMENT_LDTS TIMESTAMP,
	PAYMENT_RSRC CHAR(256),
	CUST_HKEY CHAR(32),
	CUST_CREDIT_HKEY CHAR(32)
);

ALTER TABLE PAYMENT_LNK
	ADD CONSTRAINT PK_PAYMENT_LNK PRIMARY KEY (LNK_HSH, PAYMENT_HKEY);

CREATE TABLE PAYMENT_SAT
(
	PAYMENT_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	PAYMENT_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	PAYMENT_TRANSACTION_NUMBER DEC,
	PAYMENT_TYPE VARCHAR(8000),
	CUST_NUMBER DEC,
	CUSTOMER_NO DEC,
	PAYMENT_AMOUNT DEC,
	PAYMENT_DATE VARCHAR(8000),
	PAYMENT_STATUS VARCHAR(1),
	EMP_NUMBER VARCHAR(20),
	CHECK_BANK_NUMBER DEC,
	CHECK_NUMBER DEC,
	EPAY_VENDOR_NUMBER DEC,
	EPAY_ACCOUNT_NUMBER DEC,
	CREDIT_CARD_NUMBER VARCHAR(8000),
	CREDIT_CARD_EXP VARCHAR(8000),
	CREDIT_CARD_TYPE VARCHAR(8000),
	PAYMENT_HUB_HKEY CHAR(32) NOT NULL,
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	PAYMENT_SAT_RSRC CHAR(256)
);

ALTER TABLE PAYMENT_SAT
	ADD CONSTRAINT PK_PAYMENT_SAT PRIMARY KEY (LOAD_DTS, PAYMENT_HSH, PAYMENT_HUB_HKEY, LDTS, RSRC);

CREATE TABLE PURCHASE_REF
(
	DATE_ID DEC NOT NULL,
	HOUR DEC NOT NULL,
	TITLE_ID DEC NOT NULL,
	PLATFORM_ID DEC NOT NULL,
	NUCLEUS_PERSONA_KEY DEC NOT NULL,
	NUCLEUS_USER_KEY DEC NOT NULL,
	RELEASE_TYPE_ID DEC NOT NULL,
	ECONOMY_EVENT_ID DEC NOT NULL,
	CURRENCY_OUT_ID DEC NOT NULL,
	CURRENCY_IN_ID DEC NOT NULL,
	ITEM_ID DEC NOT NULL,
	TRANSACTION_TYPE_ID DEC NOT NULL,
	ACCOUNT_LEVEL DEC,
	TRANSACTION_KEY VARCHAR(128) NOT NULL,
	TRANSACTION_SUB_TYPE_KEY VARCHAR(50),
	SESSION_KEY VARCHAR(50),
	CURRENCY_IN_QUANTITY DEC,
	USD_QUANTITY DEC,
	VAT_USD_QUANTITY DEC,
	EA_SHARE_USD_QUANTITY DEC,
	ITEM_IN_MODIFIERS BINARY,
	ITEM_IN_QUANTITY DEC,
	TRANSACTION_DATE_TIME TIMESTAMP NOT NULL,
	ATTRIBUTION_TYPE DEC NOT NULL,
	CURRENCY_OUT_QUANTITY DEC,
	DISCOUNT_ID DEC NOT NULL DEFAULT 1 ,
	REVENUE_MODEL_KEY VARCHAR(50),
	SUBSCRIPTIONS BINARY,
	FIRST_PARTY_REVENUE_SHARE_ID DEC NOT NULL DEFAULT 1 ,
	LAUNCHER_ID DEC NOT NULL DEFAULT 1 ,
	SOURCE_ID DEC NOT NULL DEFAULT 1 
);

CREATE TABLE STORE_HUB
(
	STORE_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	STORE_NUMBER DEC,
	STORE_HKEY CHAR(32) NOT NULL,
	STORE_LDTS TIMESTAMP,
	STORE_RSRC CHAR(256)
);

ALTER TABLE STORE_HUB
	ADD CONSTRAINT PK_STORE_HUB PRIMARY KEY (STORE_HSH, STORE_HKEY);

CREATE TABLE STORE_SAT
(
	STORE_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	STORE_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	STORE_NUMBER DEC,
	STORE_MANAGER VARCHAR(20),
	STORE_ADDRESS VARCHAR(20),
	STORE_PHONE DEC,
	STORE_CITY VARCHAR(20),
	STORE_STATE VARCHAR(20),
	STORE_ZIP_CODE DEC,
	STORE_ADDRESS_2 VARCHAR(20),
	STORE_HKEY CHAR(32) NOT NULL,
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	STORE_SAT_RSRC CHAR(256)
);

ALTER TABLE STORE_SAT
	ADD CONSTRAINT PK_STORE_SAT PRIMARY KEY (LOAD_DTS, STORE_HSH, STORE_HKEY, LDTS, RSRC);

ALTER TABLE CUST_CREDIT_SAT
	ADD CONSTRAINT FK_CUST_CREDIT_SAT_CUST_CREDIT FOREIGN KEY (CUST_CREDIT_HSH, CUST_CREDIT_HKEY) REFERENCES CUST_CREDIT_HUB (CUST_CREDIT_HSH, CUST_CREDIT_HKEY);

ALTER TABLE CUST_SAT
	ADD CONSTRAINT FK_CUST_SAT_CUST_HUB FOREIGN KEY (CUST_HSH, CUST_HKEY) REFERENCES CUST_HUB (CUST_HSH, CUST_HKEY);

ALTER TABLE EMP_LNK
	ADD CONSTRAINT FK_EMP_LNK_EMP_HUB FOREIGN KEY (EMP_HSH) REFERENCES EMP_HUB (EMP_HSH);

ALTER TABLE EMP_LNK
	ADD CONSTRAINT FK_EMP_LNK_STORE_HUB FOREIGN KEY (STORE_HSH, STORE_HKEY) REFERENCES STORE_HUB (STORE_HSH, STORE_HKEY);

ALTER TABLE EMP_SAT
	ADD CONSTRAINT FK_EMP_SAT_EMP_HUB FOREIGN KEY (EMP_HSH) REFERENCES EMP_HUB (EMP_HSH);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT FK_MO_RENT_REC_LNK_CUST_CREDIT FOREIGN KEY (CUST_CREDIT_HSH, CUST_CREDIT_HKEY) REFERENCES CUST_CREDIT_HUB (CUST_CREDIT_HSH, CUST_CREDIT_HKEY);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT FK_MO_RENT_REC_LNK_CUST_HUB FOREIGN KEY (CUST_HSH, CUST_HKEY) REFERENCES CUST_HUB (CUST_HSH, CUST_HKEY);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT FK_MO_RENT_REC_LNK_EMP_HUB FOREIGN KEY (EMP_HSH) REFERENCES EMP_HUB (EMP_HSH);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT FK_MO_RENT_REC_LNK_MOVIE_COPY_ FOREIGN KEY (MOVIE_COPY_HSH, MOVIE_COPY_HKEY) REFERENCES MOVIE_COPY_HUB (MOVIE_COPY_HSH, MOVIE_COPY_HKEY);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT FK_MO_RENT_REC_LNK_MO_RENT_REC FOREIGN KEY (MO_RENT_REC_HSH) REFERENCES MO_RENT_REC_HUB (MO_RENT_REC_HSH);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT FK_MO_RENT_REC_LNK_PAYMENT_HUB FOREIGN KEY (PAYMENT_HSH, PAYMENT_HUB_HKEY) REFERENCES PAYMENT_HUB (PAYMENT_HSH, PAYMENT_HUB_HKEY);

ALTER TABLE MO_RENT_REC_SAT
	ADD CONSTRAINT FK_MO_RENT_REC_SAT_MO_RENT_REC FOREIGN KEY (MO_RENT_REC_HSH) REFERENCES MO_RENT_REC_HUB (MO_RENT_REC_HSH);

ALTER TABLE MOVIE_COPY_LNK
	ADD CONSTRAINT FK_MOVIE_COPY_LNK_MOVIE_COPY_H FOREIGN KEY (MOVIE_COPY_HSH, MOVIE_COPY_HKEY) REFERENCES MOVIE_COPY_HUB (MOVIE_COPY_HSH, MOVIE_COPY_HKEY);

ALTER TABLE MOVIE_COPY_LNK
	ADD CONSTRAINT FK_MOVIE_COPY_LNK_MOVIE_HUB FOREIGN KEY (MOVIE_HSH, MOVIE_HKEY) REFERENCES MOVIE_HUB (MOVIE_HSH, MOVIE_HKEY);

ALTER TABLE MOVIE_COPY_SAT
	ADD CONSTRAINT FK_MOVIE_COPY_SAT_MOVIE_COPY_H FOREIGN KEY (MOVIE_COPY_HSH, MOVIE_COPY_HKEY) REFERENCES MOVIE_COPY_HUB (MOVIE_COPY_HSH, MOVIE_COPY_HKEY);

ALTER TABLE MOVIE_SAT
	ADD CONSTRAINT FK_MOVIE_SAT_MOVIE_HUB FOREIGN KEY (MOVIE_HSH, MOVIE_HKEY) REFERENCES MOVIE_HUB (MOVIE_HSH, MOVIE_HKEY);

ALTER TABLE MOVIE_STORE_LNK
	ADD CONSTRAINT FK_MOVIE_STORE_LNK_MOVIE_HUB FOREIGN KEY (MOVIE_HSH, MOVIE_HKEY) REFERENCES MOVIE_HUB (MOVIE_HSH, MOVIE_HKEY);

ALTER TABLE MOVIE_STORE_LNK
	ADD CONSTRAINT FK_MOVIE_STORE_LNK_STORE_HUB FOREIGN KEY (STORE_HSH, STORE_HKEY) REFERENCES STORE_HUB (STORE_HSH, STORE_HKEY);

