	MO_RENT_REC_RSRC CHAR(256)
);

ALTER TABLE MO_RENT_REC_HUB
	ADD CONSTRAINT PK_MO_RENT_REC_HUB PRIMARY KEY (MO_RENT_REC_HSH);

CREATE TABLE MO_RENT_REC_LNK
(
	LNK_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000),
	REC_SRC VARCHAR(255),
	MOVIE_COPY_HSH VARCHAR(8000),
	MO_RENT_REC_HSH VARCHAR(8000),
	EMP_HSH VARCHAR(8000),
	CUST_HSH VARCHAR(8000),
	PAYMENT_HSH VARCHAR(8000),
	CUST_CREDIT_HSH VARCHAR(8000),
	PAYMENT_HUB_HKEY CHAR(32),
	MOVIE_COPY_HKEY CHAR(32),
	MO_RENT_REC_HKEY CHAR(32) NOT NULL,
	MO_RENT_REC_LDTS TIMESTAMP,
	MO_RENT_REC_RSRC CHAR(256),
	CUST_HKEY CHAR(32),
	CUST_CREDIT_HKEY CHAR(32)
);

ALTER TABLE MO_RENT_REC_LNK
	ADD CONSTRAINT PK_MO_RENT_REC_LNK PRIMARY KEY (LNK_HSH, MO_RENT_REC_HKEY);

CREATE TABLE MO_RENT_REC_SAT
(
	MO_RENT_REC_HSH VARCHAR(8000) NOT NULL,
	LOAD_DTS VARCHAR(8000) NOT NULL,
	MO_RENT_REC_HSH_DIFF VARCHAR(8000),
	REC_SRC VARCHAR(255),
	RENTAL_RECORD_DATE VARCHAR(8000),
	MO_CO_NUM DEC,
	MOVIE_NUMBER DEC,
	SOC_SEC_NUMBER DEC,
	EMP_PHONE DEC,
	CUST_NUMBER DEC,
	RENTAL_DATE VARCHAR(8000),
	DUE_DATE VARCHAR(8000),
	RENTAL_STATUS VARCHAR(20),
	OVERDUE_CHARGE DEC,
	RENTAL_RATE DEC,
	PAYMENT_TRANSACTION_NUMBER DEC,
	CUST_NUMBER2 DEC,
	LDTS CHAR(32) NOT NULL,
	RSRC TIMESTAMP NOT NULL,
	MO_RENT_REC_SAT_RSRC CHAR(256)
);

ALTER TABLE MO_RENT_REC_SAT
	ADD CONSTRAINT PK_MO_RENT_REC_SAT PRIMARY KEY (LOAD_DTS, MO_RENT_REC_HSH, LDTS, RSRC);

